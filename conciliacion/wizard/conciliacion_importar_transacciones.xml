<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="conciliacion_importar_transacciones_form" model="ir.ui.view">
        <field name="name">conciliacion.importar_transacciones.form</field>
        <field name="model">conciliacion.importar_transacciones</field>
        <field name="arch" type="xml">
            <form>
                <field name="bank_statement_id" invisible="1"/>
                <field name="fichero"/>
                <footer>
                    <button string="Importar" name="action_importar" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>